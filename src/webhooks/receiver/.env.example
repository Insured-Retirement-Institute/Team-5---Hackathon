# Copy this file to .env and fill in values for local development.
# The app auto-loads src/webhooks/receiver/.env on startup.

# Required for webhook signature validation
ATS_WEBHOOK_SECRET=replace-with-strong-shared-secret

# Enable DynamoDB persistence when set
DYNAMODB_TABLE_NAME=book-of-business-transfers
DYNAMODB_PK_NAME=transferId

# DynamoDB connection settings
# For AWS-hosted DynamoDB:
AWS_REGION=us-east-1

# For local DynamoDB (optional):
# DYNAMODB_ENDPOINT_URL=http://localhost:8001

# Optional AWS credentials for local execution if not already configured
# AWS_ACCESS_KEY_ID=
# AWS_SECRET_ACCESS_KEY=
# AWS_SESSION_TOKEN=
